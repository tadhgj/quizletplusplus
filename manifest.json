{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name":"Quizlet++",
    "manifest_version":3,
	"description": "Extension that modifies Quizlet for more \"features\".",
	"icons": {
		"16":"16x16.png",
		"48":"48x48.png",
		"128":"128x128.png"
	},
	"short_name":"Q++",
	"version":"1.3",
	"version_name":"Beta Build 3",
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
				"https://quizlet.com/*/learn"
			],
			"js": ["jquery_3.4.1_slim_min.js","learn-inject1.js","combined.js"]
		},
		{
			"matches": [
				"https://quizlet.com/*/write"
			],
			"js": ["jquery_3.4.1_slim_min.js","write-inject1.js","write.js"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": [ "https://*/*" ],
			"resources": [ "learn-inject2.js" ]
		},
		{
			
			"matches": [ "https://*/*" ],
			"resources": ["write-inject2.js"]
		}
	]
}